{
  "name": "@openmgr/agent-storage",
  "version": "0.1.0",
  "description": "Session and message management layer for @openmgr/agent",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "@openmgr/agent-core": "workspace:*",
    "@openmgr/agent-database": "workspace:*"
  },
  "devDependencies": {
    "typescript": "^5.8.3",
    "vitest": "^4.0.0"
  },
  "keywords": [
    "openmgr",
    "agent",
    "storage",
    "sqlite",
    "drizzle"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/openmgr/agent.git",
    "directory": "packages/storage"
  }
}
