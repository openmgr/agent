{
  "name": "@openmgr/agent",
  "version": "0.1.0",
  "description": "OpenMgr Agent - AI coding assistant with batteries included",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "bin": {
    "openmgr-agent": "./dist/bin.js",
    "oa": "./dist/bin.js"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./core": {
      "types": "./node_modules/@openmgr/agent-core/dist/index.d.ts",
      "import": "@openmgr/agent-core"
    },
    "./providers": {
      "types": "./node_modules/@openmgr/agent-providers/dist/index.d.ts",
      "import": "@openmgr/agent-providers"
    },
    "./tools": {
      "types": "./node_modules/@openmgr/agent-tools/dist/index.d.ts",
      "import": "@openmgr/agent-tools"
    },
    "./tools-terminal": {
      "types": "./node_modules/@openmgr/agent-tools-terminal/dist/index.d.ts",
      "import": "@openmgr/agent-tools-terminal"
    },
    "./storage": {
      "types": "./node_modules/@openmgr/agent-storage/dist/index.d.ts",
      "import": "@openmgr/agent-storage"
    },
    "./memory": {
      "types": "./node_modules/@openmgr/agent-memory/dist/index.d.ts",
      "import": "@openmgr/agent-memory"
    },
    "./auth-anthropic": {
      "types": "./node_modules/@openmgr/agent-auth-anthropic/dist/index.d.ts",
      "import": "@openmgr/agent-auth-anthropic"
    },
    "./skills-bundled": {
      "types": "./node_modules/@openmgr/agent-skills-bundled/dist/index.d.ts",
      "import": "@openmgr/agent-skills-bundled"
    },
    "./lsp": {
      "types": "./node_modules/@openmgr/agent-lsp/dist/index.d.ts",
      "import": "@openmgr/agent-lsp"
    },
    "./server": {
      "types": "./node_modules/@openmgr/agent-server/dist/index.d.ts",
      "import": "@openmgr/agent-server"
    },
    "./cli": {
      "types": "./node_modules/@openmgr/agent-cli/dist/index.d.ts",
      "import": "@openmgr/agent-cli"
    },
    "./database": {
      "types": "./node_modules/@openmgr/agent-database/dist/index.d.ts",
      "import": "@openmgr/agent-database"
    }
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "@openmgr/agent-core": "workspace:*",
    "@openmgr/agent-providers": "workspace:*",
    "@openmgr/agent-tools": "workspace:*",
    "@openmgr/agent-tools-terminal": "workspace:*",
    "@openmgr/agent-database": "workspace:*",
    "@openmgr/agent-storage": "workspace:*",
    "@openmgr/agent-memory": "workspace:*",
    "@openmgr/agent-auth-anthropic": "workspace:*",
    "@openmgr/agent-skills-bundled": "workspace:*",
    "@openmgr/agent-lsp": "workspace:*",
    "@openmgr/agent-server": "workspace:*",
    "@openmgr/agent-cli": "workspace:*"
  },
  "devDependencies": {
    "@types/node": "^22.19.7",
    "typescript": "^5.6.0"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "keywords": [
    "ai",
    "agent",
    "coding",
    "assistant",
    "claude",
    "openai",
    "llm"
  ]
}
